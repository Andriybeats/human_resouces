<%= form_with scope: :vacation, url: vacations_path, local: true do |form| %>
  <p>
    <%= form.label :employee %><br>
    <%= form.text_field :employee %>
  </p>
  <p>
    <%= form.label :date_start %><br>
    <%= form.text_field :date_start %>
  </p>
  <p>
    <%= form.label :date_end %><br>
    <%= form.text_field :date_end %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
